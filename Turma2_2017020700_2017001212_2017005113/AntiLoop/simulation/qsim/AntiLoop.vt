`timescale 1 ps/ 1 ps
module AntiLoop_vlg_sample_tst(
	clk,
	contador,
	entrada,
	sampler_tx
);
input  clk;
input [7:0] contador;
input [15:0] entrada;
output sampler_tx;

reg sample;
time current_time;
always @(clk or contador or entrada)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

